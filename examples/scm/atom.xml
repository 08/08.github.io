<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>SCM Examples</title>
  <link href="http://08.github.io/examples/scm/atom.xml" rel="self" />
  <link href="http://08.github.io/examples/scm/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://08.github.io/examples/scm/</id>
  <author>
    <name>Ethan</name>
    <email>test@rippletc.com</email>
  </author>
  {% for example in site.categories.scm %}
  <entry>
    <title>{{ example.title }}</title>
    <link href="http://08.github.io{{ example.url }}" />
    <updated>{{ example.date | date_to_xmlchema }}</updated>
    <id>http://08.github.io{{ example.url }}</id>
    <content type="html">{{ example.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
